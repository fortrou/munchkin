<?xml version='1.0' standalone='yes'?>
<config>
	<routers>
		<cabinet>
			<controller>
				<initController>CabinetController</initController>
				<decks>
					<initAction>get_decksTemplate</initAction>
				</decks>
				<deck_create>
					<initAction>get_decksCreateTemplate</initAction>
				</deck_create>
				<cards>
					<initAction>get_cardsTemplate</initAction>
				</cards>
				<card_create>
					<initAction>get_cardsCreateTemplate</initAction>
				</card_create>
				<user-list>
					<initAction>get_usersListTemplate</initAction>
				</user-list>
			</controller>
		</cabinet>
		<game>
			<controller>
				<initController></initController>
				<actionAlias>
					<initAction></initAction>
				</actionAlias>
			</controller>
		</game>
		<authorization>
			<controller>
				<initController>AuthorizationController</initController>
				<login>
					<initAction>get_loginTemplate</initAction>
				</login>
				<registration>
					<initAction>get_registerTemplate</initAction>
				</registration>
				<sign_out>
					<initAction>deauthorize_user</initAction>
				</sign_out>
			</controller>
		</authorization>
	</routers>
</config>